[WebPagetest]
url=http://staging.webpagetest.org/
location=Dev_Pat
browser=chrome
;debug=1
;web_page_replay_host=192.168.0.11

[chrome]
exe="C:\Program Files (x86)\Google\Chrome\Application\chrome.exe"
options='--load-extension="%WPTDIR%\extension" --user-data-dir="%WPTDIR%\chrome_user_data" --new-window --no-proxy-server --no-first-run --no-default-browser-check'
cache="%WPTDIR%\chrome_user_data"

[firefox]
exe="C:\Program Files (x86)\Mozilla Firefox\firefox.exe"
options='-profile="%WPTDIR%\firefox_profile" -no-remote'
setup_cmd='C:\Python26\python.exe "%WPTDIR%\firefox\profile_setup.py" --profile_zip="%WPTDIR%\firefox\firefox5_profile.zip" --profile_dir="%WPTDIR%\firefox_profile" --extension_dir="%WPTDIR%\firefox\extension"'
